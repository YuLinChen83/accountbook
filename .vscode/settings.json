{
  "editor.formatOnSave": true,
  "files.autoSave": "onWindowChange",
  "editor.tabSize": 2,
  "editor.rulers": [80, 120],
  "editor.wordWrapColumn": 120,
  "editor.wordWrap": "wordWrapColumn",
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "html"
  },
  "html.format.wrapLineLength": 120,
  "html.format.wrapAttributes": "force-aligned",
  "javascript.validate.enable": false,
  "vetur.format.styleInitialIndent": true,
  "vetur.format.scriptInitialIndent": true,
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatterOptions": {
    "prettier": {
      "printWidth": 120,
      "semi": false,
      "eslintIntegration": false,
      "singleQuote": true
    },
    "js-beautify-html": {
      "wrap_line_length": 120,
      "wrap_attributes": "force-aligned"
    }
  },
  "blueprint.templatesPath": [".vscode/blueprint-templates"],
  "cSpell.words": ["mixins", "LIFECYCLE", "vuex"],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/yarn.lock",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    ".vscode"
  ],
  "codemetrics.basics.ComplexityLevelNormal": 10,
  "codemetrics.basics.ComplexityLevelHigh": 20,
  "codemetrics.basics.ComplexityLevelExtreme": 30
}
